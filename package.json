{
  "name": "tiltakspenger-meldekort-microfrontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "mock": "tsx watch mock/server.ts",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "prettier": "prettier --write './src/**/*.{ts,tsx,astro}'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.4.3",
    "@astrojs/react": "^4.1.2",
    "@navikt/aksel-icons": "7.27.0",
    "@navikt/ds-css": "7.27.0",
    "@navikt/ds-react": "7.28.1",
    "@navikt/nav-dekoratoren-moduler": "3.4.0",
    "@navikt/oasis": "^3.4.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "astro": "^5.1.1",
    "pino": "^9.7.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@hono/node-server": "1.11.2",
    "@types/node": "24.3.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-css-modules": "^2.12.0",
    "globals": "^16.3.0",
    "hono": "^4.9.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.3",
    "postcss-prefix-selector": "^2.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "rollup-plugin-import-map": "3.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "semi": true,
    "tabWidth": 4,
    "printWidth": 100,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint"
    ],
    "*.{ts,tsx,css,astro}": [
      "prettier --write"
    ]
  }
}
