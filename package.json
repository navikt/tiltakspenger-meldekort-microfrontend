{
  "name": "tiltakspenger-meldekort-microfrontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "mock": "tsx watch mock/server.ts",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "prettier": "prettier --write './src/**/*.{ts,tsx,astro}'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/node": "^9.5.1",
    "@astrojs/react": "^4.4.2",
    "@navikt/aksel-icons": "8.2.0",
    "@navikt/ds-css": "8.2.0",
    "@navikt/ds-react": "8.2.0",
    "@navikt/nav-dekoratoren-moduler": "3.6.1",
    "@navikt/oasis": "^4.1.1",
    "@opentelemetry/sdk-node": "^0.208.0",
    "astro": "^5.16.4",
    "pino": "^10.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@hono/node-server": "1.19.7",
    "@types/node": "25.0.3",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "globals": "^16.5.0",
    "hono": "^4.11.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.3",
    "postcss-prefix-selector": "^2.1.1",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "rollup-plugin-import-map": "3.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "semi": true,
    "tabWidth": 4,
    "printWidth": 100,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint"
    ],
    "*.{ts,tsx,css,astro}": [
      "prettier --write"
    ]
  }
}
